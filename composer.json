{
  "name": "ccu-plus/api-service",
  "description": "CCU PLUS API Service.",
  "keywords": ["ccu-plus"],
  "version": "1.0.4",
  "license": "AGPL-3.0-only",
  "type": "project",
  "authors": [
    {
      "name": "bepsvpt",
      "email": "og7lsrszah6y3lz@infinitefa.email"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-intl": "*",
    "ext-json": "*",
    "algolia/algoliasearch-client-php": "^4.19",
    "bepsvpt/secure-headers": "^9.0",
    "doctrine/dbal": "^4.2",
    "gregwar/captcha": "^1.2",
    "guzzlehttp/guzzle": "^7.9",
    "illuminate/redis": "^11.45",
    "laravel/lumen-framework": "^11.2",
    "laravel/scout": "^10.15",
    "laravel/tinker": "^2.10",
    "predis/predis": "^3.0",
    "spatie/laravel-fractal": "^6.3",
    "vlucas/phpdotenv": "^5.6",
    "thepixeldeveloper/sitemap": "^5.1"
  },
  "require-dev": {
    "rector/rector": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "rector": "rector process --memory-limit=-1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
