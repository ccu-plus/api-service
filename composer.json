{
  "name": "ccu-plus/api-service",
  "description": "CCU PLUS API Service.",
  "keywords": ["ccu-plus"],
  "version": "1.0.4",
  "license": "AGPL-3.0-only",
  "type": "project",
  "authors": [
    {
      "name": "bepsvpt",
      "email": "og7lsrszah6y3lz@infinitefa.email"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-intl": "*",
    "ext-json": "*",
    "algolia/algoliasearch-client-php": "~2.5",
    "bepsvpt/secure-headers": "~6.0",
    "doctrine/dbal": "~2.10",
    "gregwar/captcha": "~1.1",
    "guzzlehttp/guzzle": "~6.5",
    "illuminate/redis": "~6.8",
    "laravel/lumen-framework": "~6.3",
    "laravel/scout": "~7.2",
    "laravel/tinker": "~2.0",
    "predis/predis": "~1.1",
    "spatie/laravel-fractal": "~5.6",
    "vlucas/phpdotenv": "~3.6",
    "thepixeldeveloper/sitemap": "~5.1"
  },
  "require-dev": {
    "fruitcake/laravel-cors": "~1.0",
    "fzaninotto/faker": "~1.9",
    "phpunit/phpunit": "~9.0",
    "mockery/mockery": "~1.3",
    "rector/rector": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "rector": "rector process --memory-limit=-1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
